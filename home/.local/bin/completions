#!/bin/bash
# :module: Handles shell command completions setup

# add completions to this folder with format _example
# e.g dotty completion zsh > "${ZDOTDIR}/completions/_dotty"
# don't forget to run compinit after

volta completions zsh >"${ZDOTDIR}/completions/_volta"
dotty completion zsh >"${ZDOTDIR}/completions/_dotty"
